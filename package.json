{
  "name": "fiber",
  "description": "Give your Backbone Apps some Fiber.",
  "url": "http://familydev.in.ua/hub/fiber",
  "keywords": [
    "backbone",
    "marionette",
    "model",
    "view",
    "controller",
    "router",
    "server",
    "client",
    "browser"
  ],
  "author": "Igor Krimerman",
  "devDependencies": {
    "chai": "^3.5.0",
    "docco": "^0.7.0",
    "eslint": "^1.10.3",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.3.1",
    "gulp-eslint": "^1.1.1",
    "gulp-inject": "^3.0.0",
    "gulp-mocha": "^2.2.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-opera-launcher": "^0.3.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "mocha": "^2.4.5",
    "mr-doc-theme-cayman": "^1.0.3",
    "shelljs": "^0.6.0",
    "sinon": "^1.17.3",
    "uglify-js": "^2.6.1"
  },
  "dependencies": {
    "backbone": "^1.2.3",
    "combyne": "^0.8.1",
    "jquery": "^2.2.0",
    "lodash": "^4.5.0"
  },
  "scripts": {
    "build": "gulp compile",
    "min": "uglifyjs ./build/fiber.js --mangle --source-map ./build/fiber-min.map -o ./build/fiber-min.js",
    "lint": "eslint fiber.js",
    "test": "./node_modules/mocha/bin/mocha",
    "doc": "npm run clearDocs && docco ./build/fiber.js -o ./docs/docco && open -a \"Google Chrome\"  docs/docco/fiber.html",
    "jsdoc": "npm run clearDocs && jsdoc build/fiber.js -c ./jsdoc.conf.json -d docs/jsdoc --verbose",
    "jsdocAll": "npm run clearDocs && jsdoc build/fiber.js -c ./jsdoc.conf.json -P ./package.json -R ./README.md -d docs/jsdoc --verbose",
    "clearDocs": "rm -rf ./docs"
  },
  "main": "fiber.js",
  "version": "0.0.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imkrimerman/fiber.git"
  },
  "files": [
    "fiber.js",
    "fiber-min.js",
    "fiber-min.map",
    "LICENSE"
  ]
}
